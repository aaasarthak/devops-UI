<mvc:View controllerName="Devops.EmployeeMgmtSystem.controller.HomePage" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
			<App id="app">
				<Page id="page" title="{i18n>title}" showHeader="true" enableScrolling="true">
						<TileContainer id="container" tiles="{/TileCollection}">
							<StandardTile icon="{icon}" type="{type}" number="{number}" numberUnit="{numberUnit}" title="{title}" info="{info}" infoState="{infoState}"
								press="handleTilePress"/>
						</TileContainer>
				</Page>
			</App>
				</mvc:View>
